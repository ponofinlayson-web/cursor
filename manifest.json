{
  "manifest_version": 3,
  "name": "Grouped Links - Modern Homepage",
  "version": "1.2.12",
  "description": "A beautiful, modern homepage with Google search integration, dynamic favicons, organized link groups, and theme customization.",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
